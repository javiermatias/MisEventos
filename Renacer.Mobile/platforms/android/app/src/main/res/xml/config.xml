<?xml version='1.0' encoding='utf-8'?>
<widget id="com.miseventos.renacer.mobile" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>misEventosMobile</name>
    <description>
       Mis Eventos - Renacer
    </description>
    <author email="augustogalan7@gmail.com" href="http://cordova.io">
        MisEventos Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <icon density="ldpi" src="az_logo.png" />
    <icon density="mdpi" src="az_logo.png" />
    <icon density="hdpi" src="az_logo.png" />
    <icon density="xhdpi" src="az_logo.png" />
    <icon density="xxhdpi" src="az_logo.png" />
    <icon density="xxxhdpi" src="az_logo.png" />
    <preference name="loglevel" value="DEBUG" />
</widget>
