{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "misEventos": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "D:/release_miseventos/dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "src/tsconfig.app.json",
            "polyfills": "src/polyfills.ts",
            "assets": [
              "src/assets",
              "src/favicon.ico"
            ],
            "styles": [
              "node_modules/font-awesome/scss/font-awesome.scss",
              "node_modules/bootstrap/scss/bootstrap.scss",
              "node_modules/socicon/css/socicon.css",
              "node_modules/metrojs/release/MetroJs.Full/MetroJs.css",
              "node_modules/dragula/dist/dragula.min.css",
              "node_modules/ngx-toastr/toastr.css",
              "src/styles.scss"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/jquery-ui-dist/jquery-ui.min.js",
              "node_modules/tether/dist/js/tether.min.js",
              "node_modules/bootstrap/dist/js/bootstrap.min.js",
              "node_modules/skycons/skycons.js",
              "node_modules/jquery-knob/dist/jquery.knob.min.js",
              "node_modules/d3/d3.min.js",
              "node_modules/topojson/dist/topojson.min.js",
              "node_modules/datamaps/dist/datamaps.all.hires.min.js",
              "node_modules/moment/moment.js",
              "node_modules/fullcalendar/dist/fullcalendar.min.js",
              "node_modules/fullcalendar/dist/locale/es.js",
              "node_modules/dropzone/dist/min/dropzone.min.js",
              "node_modules/jspdf/dist/jspdf.min.js",
              "node_modules/jspdf-autotable/dist/jspdf.plugin.autotable.js"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "misEventos:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "misEventos:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "misEventos:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/jquery-ui-dist/jquery-ui.min.js",
              "node_modules/tether/dist/js/tether.min.js",
              "node_modules/bootstrap/dist/js/bootstrap.min.js",
              "node_modules/skycons/skycons.js",
              "node_modules/jquery-knob/dist/jquery.knob.min.js",
              "node_modules/d3/d3.min.js",
              "node_modules/topojson/dist/topojson.min.js",
              "node_modules/datamaps/dist/datamaps.all.hires.min.js",
              "node_modules/moment/moment.js",
              "node_modules/fullcalendar/dist/fullcalendar.min.js",
              "node_modules/fullcalendar/dist/locale/es.js",
              "node_modules/dropzone/dist/min/dropzone.min.js",
              "node_modules/jspdf/dist/jspdf.min.js",
              "node_modules/jspdf-autotable/dist/jspdf.plugin.autotable.js"
            ],
            "styles": [
              "node_modules/font-awesome/scss/font-awesome.scss",
              "node_modules/bootstrap/scss/bootstrap.scss",
              "node_modules/socicon/css/socicon.css",
              "node_modules/metrojs/release/MetroJs.Full/MetroJs.css",
              "node_modules/dragula/dist/dragula.min.css",
              "node_modules/ngx-toastr/toastr.css",
              "src/styles.scss"
            ],
            "assets": [
              "src/assets",
              "src/favicon.ico"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": []
          }
        }
      }
    },
    "misEventos-e2e": {
      "root": "e2e",
      "sourceRoot": "e2e",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "misEventos:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "e2e/tsconfig.e2e.json"
            ],
            "exclude": []
          }
        }
      }
    }
  },
  "defaultProject": "misEventos",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "az",
      "styleext": "scss",
      "spec": false
    },
    "@schematics/angular:directive": {
      "prefix": "az"
    }
  }
}